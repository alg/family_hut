%h1= t(".header")

.commands
  = link_to t(".new_album"), new_album_path

#news
  %h2= t(".news")

  = render :partial => "shared/event", :collection => @events

#albums
  %h2= t(".my_albums")

  - if @albums.empty?
    .no-items= t(".no_albums")
  - else
    = render :partial => "albums/album", :collection => @albums

