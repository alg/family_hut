%h1 Dashboard

.commands
  = link_to "New Album", new_album_path

#news
  %h2 News

  = render :partial => "shared/event", :collection => @events

#my_albums
  %h2 My Albums

  - if @albums.empty?
    .no-items You don't have any albums yet
  - else
    = render :partial => "albums/album", :collection => @albums

